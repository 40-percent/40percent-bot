{
  "name": "keeb-bot",
  "version": "0.0.1",
  "author": "Natsuki Takahari",
  "description": "A bot for the 40s mechanical keyboard Discord server.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && node build/server.js",
    "watch": "concurrently \"tsc -w\" \"nodemon -e js build/server.js\"",
    "postinstall": "tsc"
  },
  "dependencies": {
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.14.6",
    "@types/ws": "^7.2.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": "14.15.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
